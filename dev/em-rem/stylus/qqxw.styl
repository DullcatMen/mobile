body
	header
		width 20rem
		height 2.75rem
		background-image url(../../../img/headerbg.png)
		background-size 8.6rem
		background-position 139px
		background-repeat no-repeat
		position relative
		overflow hidden
		.logo_sport
			display block
			width 7.5rem
			height 1.875rem
			background-image url(../../../img/logo_sports.png)
			background-size cover
			background-repeat no-repeat
			margin-left 0.4375rem
			margin-top 0.5rem
		.nav_icon
			display block
			width 1.5rem
			height 1.125rem
			background-image url(http://mat1.gtimg.com/www/mobi/image/icon.png)
			background-size 7.4375rem 1.3125rem
			background-repeat no-repeat
			background-position -1.25rem 0
			position absolute
			top 0.7rem
			right 1rem
	.nav_btn
		width 20rem
		height 2.5rem
		background-color rgb(62,134,206)
		padding .3125rem 0
		position relative
		&:before
			position absolute
			top 0
			transform scaleY(.5)
			width 20rem
			display block
			border-bottom .0625rem solid rgb(49,126,203)
			content ""
		.btn
			display block
			float left
			width 4.125rem
			height 1.875rem
			font-size 1.125rem
			text-align center
			line-height 1.875rem
			color #fff
			&.click
				background-color rgb(34,111,187)
	.imgBox
		width 20rem
		height 11rem
		background-image url(../../../img/1.jpg)
		background-size cover
		background-repeat no-repeat
		position relative
		.img_title
			color #fff
			font-size 1rem
			position absolute
			bottom .625rem
			left .625rem
	.main
		width 20rem
		padding .3125rem
		overflow hidden
		.ranking
			width 100%
			height 3.625rem
			background-color #fff
			border .0625rem solid rgb(205,222,240)
			.top
				width 100%
				height 1.5625rem
				line-height 1.5625rem
				background-image url(../../../img/topbg.png)
				background-size cover
				background-repeat no-repeat
				color rgb(69,106,206)
				font-size .875rem
				font-weight bold
				text-indent .375rem
		.activity
			width 100%
			height 2.125rem
			background-color rgb(237,250,255)
			border .0625rem solid rgb(205,222,240)
			margin-top .3125rem
			margin-bottom 1rem
			.act_icon
				float left
				width 2.75rem
				height 1.8rem
				background-image url(../../../img/zsay_logo.png)
				background-size cover
				background-repeat no-repeat
				margin-left 1.25rem
			.act_text
				float left
				margin-left .3rem
				line-height 2.125rem
				font-size .875rem
				color rgb(6,149,215)
				font-weight bold
		.contentBox1
			width 100%
			padding .625rem 0
			overflow hidden
			position relative
			.more
				display block
				width 100%
				color #b4b4b4
				text-align center
				font-size 1rem
			&:after
				position absolute
				bottom 0
				// transform scaleY(.5)
				width 20rem
				display block
				border-bottom 1px solid #eceef0
				content ""
			.l-imgBox
				float left
				width 5rem
				height 3.75rem
				background-color #fff
				border .0625rem solid #e0e0e0
				padding .0625rem .1875rem
				img
					width 100%
					height 100%
			h1
				display block
				width 13.75rem
				float left
				font-size .9375rem
				white-space nowrap
				overflow hidden
				text-overflow ellipsis
				margin-left .3125rem
			.text
				width 13.75rem
				float left
				color #878c92
				font-size .8125rem
				margin-left .3125rem
				margin-top .5rem
				line-height 1.3rem
				overflow hidden
				position relative
				.comment
					display block
					float right
					font-size .6875em
					color #878c92
					&:before
						position relative
						top .25rem
						margin-right 3px
						content ""
						width .625rem
						height .625rem
						float left
						background-image url(http://mat1.gtimg.com/www/mobi/image/icon.png)
						background-size 7.4375rem 1.3125rem
						background-repeat no-repeat
						background-position -5.25rem 0
				&.caption
					&:after
						display block
						content ""
						width 1.5rem
						height .875rem
						float right
						margin-right .9375rem
						margin-top .125rem
						background-image url(http://mat1.gtimg.com/www/mobi/image/flagicon.png)
						background-size 10.5rem .875rem
						background-repeat no-repeat
						background-position -4.5rem 0
			&.group
				h1
					width 100%
				img
					width 100%
					margin-top .625rem
				.text
					width 20rem
					.comment
						position relative
						right 1.25rem
	.nav
		width 20rem
		padding-top 1rem
		overflow hidden
		a
			display block
			float left
			width 20%
			text-align center
			margin-bottom 1rem
		&.gary
			background-color #49535d
			a
				font-size .9375em
				color #95a0ac
		&.blue
			background-color #4998e7
			a
				font-size 1.125rem
				color #fff
	.footer
		width 20rem
		overflow hidden
		background-color #404a54
		padding 1.125rem .625rem
		color #95a0ac
		font-size .8125em
		.signIn
			margin-bottom 1rem
		.tickling
			margin-bottom .9rem
		.company
			width 100%
			white-space nowrap
			overflow hidden
			font-size .5rem
			color #707982
			
				
			
			